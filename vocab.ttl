    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

Relation:email a rdf:Property ;
    rdfs:label "email" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

Relation:numero_de_telephone a rdf:Property ;
    rdfs:label "numero_de_telephone" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

